{%- liquid
  assign items = page.children
  assign i = 0
-%}
{
  "places": [
    {%- for item in (1..3) -%}{
        "title": "Title",
        "img": "Image",
        "address": "<strong>Address</strong>",
        "description": "Description",
        "lat": "2",
        "lng": "3"
    }{%- assign i = i | plus: 1 -%}{%- if i < items.length -%},{%- endif -%}{%- endfor -%}
  ]
}
